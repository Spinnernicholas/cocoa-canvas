{
  "name": "cocoa-canvas",
  "version": "0.1.0",
  "description": "Open-source voter database and canvassing platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:unit": "vitest --include \"lib/**/*.test.{ts,tsx}\" --include \"components/**/*.test.{ts,tsx}\"",
    "test:integration": "vitest --include \"app/api/**/*.test.ts\"",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "docker:dev:up": "docker-compose -f ../docker/docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f ../docker/docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f ../docker/docker-compose.dev.yml logs -f",
    "docker:dev:restart": "docker-compose -f ../docker/docker-compose.dev.yml restart",
    "docker:prod:up": "docker-compose -f ../docker/docker-compose.yml up -d",
    "docker:prod:down": "docker-compose -f ../docker/docker-compose.yml down",
    "docker:prod:logs": "docker-compose -f ../docker/docker-compose.yml logs -f",
    "docker:prod:build": "docker-compose -f ../docker/docker-compose.yml build"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.69.3",
    "cron-parser": "^5.5.0",
    "croner": "^10.0.1",
    "csv-parse": "^6.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "next": "^16.1.6",
    "next-auth": "^4.24.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.0",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/seed.ts"
  }
}
