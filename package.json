{
  "name": "cocoa-canvas",
  "version": "0.1.0",
  "description": "Open-source voter database and canvassing platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern='(unit|lib)' --watch",
    "test:integration": "jest --testPathPattern='api' --watch",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:dev:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:prod:up": "docker-compose up -d",
    "docker:prod:down": "docker-compose down",
    "docker:prod:logs": "docker-compose logs -f",
    "docker:prod:build": "docker-compose build"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.69.3",
    "cron-parser": "^5.5.0",
    "croner": "^10.0.1",
    "csv-parse": "^6.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.6",
    "next-auth": "^4.24.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
